<?xml version="1.0" encoding="utf-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee">
    <servlet>
        <servlet-name>SystemServiceServlet</servlet-name>
        <servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
        <init-param>
            <param-name>services</param-name>
            <param-value>uk.ac.aston.wadekabs.smartcarparkapplication.backend.MyEndpoint
            </param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>SystemServiceServlet</servlet-name>
        <url-pattern>/_ah/spi/*</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>BirminghamDataFactoryServlet</servlet-name>
        <servlet-class>
            uk.ac.aston.wadekabs.smartcarparkapplication.backend.BirminghamDataFactoryServlet
        </servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>BirminghamDataFactoryServlet</servlet-name>
        <url-pattern>/BirminghamDataFactoryServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>BirminghamDataFactoryLiveServlet</servlet-name>
        <servlet-class>
            uk.ac.aston.wadekabs.smartcarparkapplication.backend.BirminghamDataFactoryLiveServlet
        </servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>BirminghamDataFactoryLiveServlet</servlet-name>
        <url-pattern>/BirminghamDataFactoryLiveServlet</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>

</web-app>